<% layout(_view.get('_layout/form')) -%>

<% block('title', 'Update photo') -%>

<% _data.set('breadcrumbs', [{
    title: 'Photos',
    url: 'admin/photo'
},{
    title: model.getId()
}]) %>

<%- partial('./form') %>

<div class="form-group">
    <label class="col-sm-2 control-label"></label>
    <div class="col-sm-10">
        <p class="form-control-static">
            <img src="<%- model.getMedium() %>" class="thumbnail img-responsive">
        </p>
    </div>
</div>