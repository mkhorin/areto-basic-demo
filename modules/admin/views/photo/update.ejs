<% layout(view.get('_layouts/form')) -%>

<% block('title', 'Update photo') -%>

<% view.setData('breadcrumbs', [
    {title: 'Photos', url: '/admin/photo'},
    {title: model.getId()}
]) %>

<% include form %>

<div class="form-group">
    <label class="col-sm-2 control-label"></label>
    <div class="col-sm-10">
        <p class="form-control-static">
            <img src="<%- model.getMedium() %>" class="thumbnail img-responsive">
        </p>
    </div>
</div>